{
    "name": "collingmedia/lasso-api",
    "description": "A PHP SDK for the Lasso CRM API.",
    "keywords": [
        "lasso",
        "crm",
        "sdk",
        "api"
    ],
    "homepage": "https://collingmedia.com",
    "license": "proprietary",
    "authors": [
        {
            "name": "Brian Logan",
            "homepage": "https://github.com/colling-media/lasso-api"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "squizlabs/php_codesniffer": "^3",
        "friendsofphp/php-cs-fixer": "^2",
        "phploc/phploc": "^5.0",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^4.1"
    },
    "autoload": {
        "psr-4": { "CollingMedia\\Lasso\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "CollingMedia\\Lasso\\" : "test/" }
    }
}
